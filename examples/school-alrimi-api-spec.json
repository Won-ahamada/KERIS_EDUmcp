      "in": "query"
    },
    "rateLimit": {
      "enabled": false,
      "description": "제한없음"
    },
    "license": {
      "type": "출처표시",
      "url": "http://www.kogl.or.kr/info/license.do#03-tab",
      "description": "출처를 표시하면 영리 목적의 이용이나 변경 및 2차적저작물의 작성을 포함한 자유 이용을 할 수 있습니다."
    }
  },
  "endpoints": [
    {
      "id": "school-basic-info",
      "apiType": "0",
      "category": "기본정보",
      "name": "학교 기본정보",
      "description": "전국 학교의 기본 정보를 조회합니다 (학교명, 주소, 연락처 등)",
      "method": "GET",
      "path": "/openApi.do",
      "parameters": [
        {
          "name": "apiKey",
          "in": "query",
          "type": "string",
          "required": true,
          "description": "인증키"
        },
        {
          "name": "apiType",
          "in": "query",
          "type": "string",
          "required": true,
          "value": "0",
          "description": "API 종류"
        },
        {
          "name": "sidoCode",
          "in": "query",
          "type": "string",
          "required": false,
          "description": "시도코드 (예: 11-서울, 26-부산)",
          "example": "11"
        },
        {
          "name": "sggCode",
          "in": "query",
          "type": "string",
          "required": false,
          "description": "시군구코드 (예: 11110-종로구)",
          "example": "11110"
