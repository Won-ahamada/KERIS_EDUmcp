  "tools": [
    {
      "name": "search_schools",
      "description": "조건에 맞는 학교를 검색합니다",
      "category": "검색",
      "inputSchema": {
        "type": "object",
        "properties": {
          "region": {
            "type": "string",
            "description": "시도명 (예: 서울특별시, 부산광역시)",
            "example": "서울특별시"
          },
          "district": {
            "type": "string",
            "description": "시군구명 (예: 강남구, 해운대구)",
            "example": "강남구"
          },
          "schoolType": {
            "type": "string",
            "enum": ["02", "03", "04", "05", "06", "07"],
            "description": "학교급 (02:초등, 03:중등, 04:고등, 05:특수, 06:그외, 07:각종)",
            "example": "04"
          },
          "schoolName": {
            "type": "string",
            "description": "학교명 검색어 (부분 일치)",
            "example": "고등학교"
          }
        },
        "required": ["schoolType"]
      },
      "outputSchema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "SCHUL_CODE": {
              "type": "string",
              "description": "학교코드"
            },
