        {
          "name": "schulKndCode",
          "in": "query",
          "type": "string",
          "required": true,
          "description": "학교급구분",
          "enum": ["02", "03", "04", "05", "06", "07"],
          "enumDescriptions": {
            "02": "초등학교",
            "03": "중학교",
            "04": "고등학교",
            "05": "특수학교",
            "06": "그외",
            "07": "각종학교"
